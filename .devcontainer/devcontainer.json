{
	"name": "eaproxy",
	"dockerComposeFile": "docker-compose.yml",
	"service": "eaproxy",
	"extensions": ["ms-vscode.go"],
	"runArgs": ["--cap-add=SYS_PTRACE", "--security-opt", "seccomp=unconfined"],
	"settings": {
		"go.gopath": "/go",
		"go.inferGopath": true,
		"go.useLanguageServer": true
	},
	"workspaceFolder": "/go/src/eaproxy"
}
